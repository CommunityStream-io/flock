              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "flock-mirage:build:production"
            },
            "development": {
              "buildTarget": "flock-mirage:build:development"
            },
            "test": {
              "buildTarget": "flock-mirage:build:test"
            },
            "test-coverage": {
              "buildTarget": "flock-mirage:build:test-coverage"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/flock-mirage/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/flock-mirage/public"
              }
            ],
            "styles": [
              "projects/flock-mirage/src/styles.scss"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/flock-mirage/**/*.ts"]
          }
        }
      }
    },
