                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "projects/flock-mirage/src/environments/environment.ts",
                  "with": "projects/flock-mirage/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            },
            "test": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/flock-mirage/src/environments/environment.ts",
                  "with": "projects/flock-mirage/src/environments/environment.test.ts"
                }
              ]
            },
            "test-coverage": {
              "optimization": false,
              "extractLicenses": false,
